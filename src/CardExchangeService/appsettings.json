{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "Host": "localhost",
    "Port": "6379",
    "GeoRadius_m": 25,
    "KeyExpireTimeout_s": 5
  },
  "ImageFileSettings": {
    "SizeLimitBytes": 512000, //500Kb
    "ThumbWidth": 150,
    "ThumbHeight": 150,
    "MaxWidth": 300,
    "MaxHeight": 300,
    "AllowedExtensions": ".jpg, .jpeg",
    "ThumbFolder": "thumbnails",
    "ImagesFolder": "images"
  }
}
